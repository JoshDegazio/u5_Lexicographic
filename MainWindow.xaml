<Window x:Class="_182685_u5_Lexicographic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_182685_u5_Lexicographic"
        mc:Ignorable="d"
        Title="u5 Lexicographic" Height="450" Width="800">
    <StackPanel>
        <Label Content="Made by Josh Degazio" Margin="10,10,10,0"/>
        <TextBox x:Name="tB_Input" Text="Enter two numbers in this box. Example: 0,2" Margin="10,10,410,0"/>
        <Button Content="Run" Margin="10,10,410,0" Click="Button_Click"/>
        <TextBlock x:Name="tB_Output" Text="Output will appear here" Margin="10,10,410,0" TextWrapping="WrapWithOverflow"/>
    </StackPanel>
</Window>
